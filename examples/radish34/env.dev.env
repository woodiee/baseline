BASELINE_MESSENGER_PROVIDER="whisper" # [ "whisper", "nats" ]
BASELINE_ZKP_MODE="0" # "0"=createMSA/createPO, "1"=dummyMSA/dummyPO
BASELINE_ETH_PROVIDER="http://ganache:8545" # used by multiple services: deploy, api, merkle-tree
BASELINE_LOG_LEVEL="debug" # [ "error", "warn", "info", "verbose", "debug", "silly" ]

# Specify where to pull @baseline-protocol core npm packages.
# Package source code is currently located at <repo_root>/baseline/core.
# If using "local", make sure to build npm packages before initializing docker containers
BASELINE_NPM_PACKAGE="local" # [ "registry", "local" ]

# Not currently used
BASELINE_NATS_JWT_SIGNER_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY----------END RSA PRIVATE KEY-----"
BASELINE_NATS_JWT_SIGNER_PUBLIC_KEY="-----BEGIN PUBLIC KEY----------END PUBLIC KEY-----"
BASELINE_ADMIN_TOKEN="cxt1234"
